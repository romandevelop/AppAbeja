<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cl.gestiona.appabeja">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono_app"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".DetalleAbejaActivity"
            android:label="@string/title_activity_detalle_abeja"
            android:parentActivityName=".HomeActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".IdentificacionActivity"
            android:label="@string/identifications"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".DetalleFamiliaActivity"
            android:label="@string/title_activity_detalle_familia"
            android:parentActivityName=".DetalleAbejaActivity"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>